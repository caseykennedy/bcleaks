{
  "name": "bc-leaks-website",
  "private": true,
  "description": "Gatsby TypeScript Starter with Styled System",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "netlify-lambda install",
    "bootstrap": "node ./scripts/bootstrap-fauna-database.js",
    "build": "gatsby build",
    "cleancache": "gatsby clean",
    "develop": "gatsby develop",
    "dev": "gatsby develop -o",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "ssr": "gatsby build && gatsby serve",
    "lint": "tslint -t stylish -p tsconfig.json",
    "lint:fix": "tslint -p tsconfig.json -t stylish --fix",
    "format": "prettier --write content/**/*.yaml",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:ci": "cypress run --browser chrome --reporter junit --reporter-options 'mochaFile=results/cypress/result.xml'",
    "precommit": "lint-staged",
    "server:open": "start-server-and-test develop http://localhost:8000 cy:open",
    "server:run": "start-server-and-test develop http://localhost:8000 cy:run",
    "server:ci": "start-server-and-test develop http://localhost:8000 cy:run:ci",
    "test:compile": "tsc --project cypress/tsconfig.json",
    "test:compile:watch": "tsc --watch --project cypress/tsconfig.json",
    "test": "cross-env CYPRESS_SUPPORT=y npm-run-all -s test:compile server:run",
    "test:watch": "cross-env CYPRESS_SUPPORT=y npm-run-all -p test:compile:watch server:open",
    "test:ci": "cross-env CYPRESS_SUPPORT=y npm-run-all -s test:compile server:ci"
  },
  "dependencies": {
    "@feuer/react-tabs": "^0.1.8",
    "@reach/dialog": "^0.10.5",
    "@reach/tabs": "^0.10.5",
    "@reach/visually-hidden": "^0.10.4",
    "@react-spring/parallax": "^9.0.0-rc.3",
    "@sanity/block-content-to-react": "^2.0.7",
    "@types/auth0-js": "^9.13.4",
    "@types/google-map-react": "^1.1.8",
    "@types/lodash": "^4.14.159",
    "@types/react-typist": "^2.0.1",
    "@types/rebass": "^3.0.4",
    "auth0-js": "^9.13.4",
    "babel-plugin-styled-components": "^1.11.1",
    "coingecko-api": "^1.0.10",
    "gatsby": "^2.24.47",
    "gatsby-image": "^2.4.16",
    "gatsby-plugin-create-client-paths": "^2.3.10",
    "gatsby-plugin-google-analytics": "^2.3.13",
    "gatsby-plugin-manifest": "^2.4.23",
    "gatsby-plugin-netlify": "^2.3.13",
    "gatsby-plugin-netlify-identity": "0.0.3",
    "gatsby-plugin-nprogress": "^2.3.10",
    "gatsby-plugin-offline": "^2.2.10",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.27",
    "gatsby-plugin-sitemap": "^2.4.11",
    "gatsby-plugin-styled-components": "^3.3.10",
    "gatsby-plugin-transition-link": "^1.20.2",
    "gatsby-plugin-typescript": "^2.4.18",
    "gatsby-source-filesystem": "^2.3.24",
    "gatsby-source-instagram": "^0.4.0",
    "gatsby-source-sanity": "^5.0.6",
    "gatsby-transformer-sharp": "^2.5.13",
    "gatsby-transformer-yaml": "^2.4.10",
    "google-map-react": "^1.1.7",
    "gsap": "^3.5.1",
    "lodash": "^4.17.20",
    "moment": "^2.27.0",
    "node-fetch": "^2.6.0",
    "polished": "^3.6.5",
    "react": "^16.13.1",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-id-swiper": "^3.0.0",
    "react-image-lightbox": "^5.1.1",
    "react-intersection-observer": "^8.26.2",
    "react-modal-hook": "^3.0.0",
    "react-moment": "^0.9.7",
    "react-netlify-identity-widget": "^0.2.7",
    "react-responsive-embed": "^2.1.0",
    "react-scroll-parallax": "^2.3.4",
    "react-spring": "^8.0.27",
    "react-ticker": "^1.2.2",
    "react-typist": "^2.0.5",
    "rebass": "^3.2.2",
    "styled-breakpoints": "^6.11.0",
    "styled-components": "^4.4.1",
    "styled-css-grid": "^1.2.1",
    "styled-system": "^4.2.4",
    "swiper": "^5.4.5",
    "typeface-work-sans": "^0.0.72",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@types/node": "^10.17.28",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^5.0.16",
    "@types/styled-components": "^4.4.3",
    "@types/styled-system": "^4.2.2",
    "cross-env": "^5.2.1",
    "cypress": "^3.8.3",
    "cypress-testing-library": "^2.6.0",
    "faunadb": "^3.0.1",
    "gatsby-cypress": "^0.1.9",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "netlify-lambda": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "start-server-and-test": "^1.11.3",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-cypress": "^1.0.4",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^4.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/caseykennedy/meno"
  },
  "bugs": {
    "url": "https://github.com/caseykennedy/meco/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.yaml": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "xo --fix",
      "git add"
    ]
  }
}
